{
  "rewrites": [
    {
      "source": "/projects",
      "destination": "/projects/index.html"
    },
    {
      "source": "/experience",
      "destination": "/experience/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
